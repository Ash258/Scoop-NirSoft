        "webbrowserpassview_lng.ini"
    ],
    "checkver": {
        "url": "https://www.nirsoft.net/pad/webbrowserpassview.xml",
        "xpath": "/XML_DIZ_INFO/Program_Info/Program_Version"
    },
    "autoupdate": {
        "url": "https://www.nirsoft.net/toolsdownload/webbrowserpassview.zip#/cosi.cosi"
    }
