        "uninstallview_lng.ini"
    ],
    "checkver": {
        "url": "https://www.nirsoft.net/pad/uninstallview.xml",
        "xpath": "/XML_DIZ_INFO/Program_Info/Program_Version"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
